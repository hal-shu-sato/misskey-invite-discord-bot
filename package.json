{
  "name": "misskey-invite-discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot to invite users to Misskey",
  "main": "build/index.js",
  "repository": "https://github.com/hal-shu-sato/misskey-invite-discord-bot.git",
  "author": "ato lash <hal-shu-sato@users.noreply.github.com>",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "axios": "^1.7.7",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@tsconfig/node16": "^16.1.3",
    "@types/node": "^22.9.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.13.0"
  },
  "scripts": {
    "test": "ts-node --esm src/index.ts",
    "compile": "tsc -p .",
    "prestart": "run-s compile",
    "start": "node build/index.js",
    "lint": "run-s lint:prettier lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint --fix-dry-run .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:prettier": "prettier --write .",
    "fix:eslint": "eslint --fix .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{ts,tsx,json,yml,md,html,css,scss}": "prettier --write"
  },
  "packageManager": "yarn@1.22.19"
}
